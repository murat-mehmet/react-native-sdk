{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "exclude": ["node_modules", "dist", "test"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "plugins/markdown",
    "node_modules/better-docs/component",
    "node_modules/better-docs/category",
    "node_modules/better-docs/typedef-import"
  ],
  "templates": {
    "cleverLinks": false,
    "search": true,
    "default": {
      "name": "Segmentify React Native SDK",
      "title": "Segmentify React Native SDK"
    },
    "better-docs": {
      "name": "Segmentify React Native SDK",
      "title": "Segmentify React Native SDK",
      "sections": [
        "Modules",
        "Classes",
        "Externals",
        "Events",
        "Namespaces",
        "Mixins",
        "Tutorials",
        "Interfaces",
        "Global"
      ],
      "logo": {
        "url": "https://segmentify.com/wp-content/themes/segmentify/dist/img/static/segmentify-header-logo.svg",
        "width": 200,
        "height": 200
      },
      "navLinks": [
        {
          "label": "Repository",
          "href": "https://github.com/segmentify/react-native-sdk"
        }
      ]
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/better-docs"
  }
}
